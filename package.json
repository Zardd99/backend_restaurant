{
  "name": "restaurant_backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server.ts",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "lint": "eslint",
    "insert-menu": "tsx scripts/insertMenuItem.ts",
    "seed-data": "tsx scripts/insertMenuItem.ts",
    "db:insert": "ts-node scripts/db/index.ts insert",
    "db:insert:file": "ts-node scripts/db/index.ts insert -f ./data/menuItems.json",
    "db:update": "ts-node scripts/db/index.ts update",
    "db:update:file": "ts-node scripts/db/index.ts update -f ./data/updates.json",
    "db:delete": "ts-node scripts/db/index.ts delete",
    "db:delete:id": "ts-node scripts/db/index.ts delete -i \"64f8c2e2b8d1e5a1c2b3a4f7\"",
    "db:delete:name": "ts-node scripts/db/index.ts delete -n \"Margherita Pizza\"",
    "db:query": "ts-node scripts/db/index.ts query",
    "db:query:vegetarian": "ts-node scripts/db/index.ts query -t vegetarian",
    "db:query:available": "ts-node scripts/db/index.ts query -a"
  },
  "type": "commonjs",
  "dependencies": {
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.9",
    "bcryptjs": "^3.0.2",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^3.0.0",
    "@types/commander": "^2.12.5",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.11",
    "@types/ws": "^8.18.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "main": "server.js",
  "keywords": [],
